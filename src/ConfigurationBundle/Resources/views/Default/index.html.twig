<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Administration WOO!!!{% endblock %}</title>
    {% block stylesheets %}
        {% stylesheets 'bundles/user/css/*' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endblock stylesheets %}
    {% block javascript %}
        {% javascripts '@UserBundle/Resources/public/js/*'
        'bundles/user/js/jquery-1.8.3.min.js'
        'bundles/user/js/jquery-ui.min.js'
        'bundles/user/js/jquery-1.9.1.js'
        %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
    {% endblock javascript %}
    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/user/img/Setting-icon.png') }}" />
</head>

<body class="size-1140">
<!-- TOP NAV WITH LOGO -->
<header>
    <nav>
        <div class="line">
            <div class="s-12 l-4">
                <img class="s-12 l-12  logo" src={{ asset('bundles/user/img/adminlogo.png') }}>
            </div>
            <div class="top-nav l-8 s-12">

            </div>
        </div>
    </nav>
</header>
<!-- ASIDE NAV AND CONTENT -->
<div class="line">
    <div class="box  margin-bottom">
        <div class="margin">
            <!-- ASIDE NAV 1 -->
            <div class="text-center">
                <h3>Administration Application!</h3>
            </div>
            <aside class="s-12 l-3">
                <h4 class="text-center"><i class="icon-settings cl-olivie"></i> Step Configuration</h4>
                <div class="aside-nav">
                    <ul>
                        <li>
                            <a>
                                {% if flag == 'welcome' %}
                                    <i class="icon-arrow_big_right margin2 cl-olivie "></i>
                                {% endif %}
                                Welcome!
                            </a>
                        </li>
                        <li>
                            <a>
                                {% if flag == 'database' %}
                                    <i class="icon-arrow_big_right margin2 cl-olivie "></i>
                                {% endif %}
                                DataBase
                            </a>
                        </li>
                        <li>
                            <a>
                                {% if flag == 'admin' %}
                                    <i class="icon-arrow_big_right margin2 cl-olivie "></i>
                                {% endif %}
                                Admin
                            </a>
                        </li>
                    </ul>
                </div>
            </aside>
            <!-- CONTENT -->
            {% block body %}
            {% endblock %}
        </div>
    </div>
</div>
<!-- FOOTER -->
<footer class="box">
    <div class="line">
        <div class="s-12 l-6">
            <p>Copyright 2017, Vision Design - graphic zoo</p>
        </div>
        <div class="s-12 l-6">
            <a class="right" href="http://www.myresponsee.com" title="Responsee - lightweight responsive framework">Design and coding by Responsee Team</a>
        </div>
    </div>
</footer>
{#<script type="text/javascript" src="js/responsee.js"></script>#}


{% block javascripts %}
    {% javascripts
    'bundles/user/js/responsee.js'
    'bundles/user/js/bootstrap.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

</body>
</html>

